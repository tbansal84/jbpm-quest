<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ProcessServlet</servlet-name>
		<display-name>ProcessServlet</display-name>
		<description></description>
		<servlet-class>org.jbpm.examples.web.ProcessServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TaskServlet</servlet-name>
		<display-name>TaskServlet</display-name>
		<description></description>
		<servlet-class>org.jbpm.examples.web.TaskServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Sample</servlet-name>
		<display-name>Sample</display-name>
		<description></description>
		<servlet-class>sample.Sample</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TaskServlet</servlet-name>
		<url-pattern>/TaskServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcessServlet</servlet-name>
		<url-pattern>/ProcessServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Sample</servlet-name>
		<url-pattern>/Sample</url-pattern>
	</servlet-mapping>
</web-app>
